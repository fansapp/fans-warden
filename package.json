{
  "name": "fans-warden",
  "version": "1.0.0",
  "description": "A data warden designed to validate an 'object' based on a ruled blueprint. Can be used for the browser's 'localStorage' or API call requests/responses. Inspired by React's PropTypes.",
  "main": "./dist/index.js",
  "directories": {
    "test": "__tests__"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha --opts mocha.opts || true",
    "build": "webpack",
    "dev": "webpack --watch",
    "lint": "eslint ./src --ext .js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "browser",
    "javascript",
    "web",
    "localStorage",
    "REST",
    "API",
    "prop-types",
    "react",
    "cookies",
    "cookie",
    "storage",
    "validation",
    "validator"
  ],
  "author": "Fans Entertainment",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.5.0",
    "mocha": "^3.5.0",
    "webpack": "^3.5.5"
  }
}
